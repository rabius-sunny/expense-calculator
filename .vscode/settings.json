{
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always",
    "source.removeUnused.ts": "always",
    "source.fixAll.prettier": "always"
  },
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true
  }
}
